{
    "version": "0.2.0",
    "configurations": [
        {"type":"node","request":"launch","name":"Run react-scripts","runtimeExecutable":"react-scripts","cwd":"${workspaceFolder}","args":[]},
      {
        "type": "pwa-chrome",
